          <stringProp name="script">import groovy.json.JsonBuilder

def json = new JsonBuilder()
Random rand = new Random()
String alphanum = ((&apos;A&apos;..&apos;Z&apos;)+(&apos;a&apos;..&apos;z&apos;)+(&apos;1&apos;..&apos;9&apos;)).join()

def iam_uid = ${uid}
def iam_display_name = iam_uid
def iam_tenant = &quot;t_&quot; + rand.nextInt(1000).toString() + ctx.getThreadNum().toString()
def iam_email = iam_uid + &quot;@seagate.com&quot;
def iam_key_type = &quot;s3&quot;
def iam_user_caps = &quot;users=*&quot;
def iam_max_buckets = rand.nextInt(1000)
def iam_generate_key = rand.nextBoolean()
def iam_suspended = rand.nextBoolean()
def iam_access_key = rand.with {(1..8).collect {alphanum[ nextInt(alphanum.length() )]}.join()}
def iam_secret_key = rand.with {(1..16).collect {alphanum[ nextInt(alphanum.length() )]}.join()}


json {
    uid iam_uid
    display_name iam_display_name
    email iam_email
    key_type iam_key_type
    access_key iam_access_key
    secret_key iam_secret_key
    user_caps iam_user_caps
    generate_key iam_generate_key
    max_buckets iam_max_buckets
    suspended iam_suspended
    tenant iam_tenant
}


log.info(&quot;-------------------------------- &quot; + json.toPrettyString())


sampler.addNonEncodedArgument(&quot;&quot;,json.toPrettyString(),&quot;&quot;)
sampler.setPostBodyRaw(true)</stringProp>
